{
  "version": "1.3.0",
  "description": "Grab bag of generic utilities and extension methods for .Net development",
  "authors": [ "Jeremy D. Miller", "Brandon Behrens", "Andrew Kharlamov" ],
  "packOptions": {
    "licenseUrl": "https://github.com/JasperFX/Baseline/blob/master/LICENSE.txt",
    "projectUrl": "https://github.com/JasperFX/Baseline"
  },
  "buildOptions": {
    "compile": [ "**/*.cs", "../CommonAssemblyInfo.cs"]
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0"
      }
    },
    "netstandard1.3": {
      "dependencies": {
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.IO.FileSystem": "4.0.1",
        "System.Linq": "4.1.0",
        "System.Linq.Expressions": "4.1.0",
        "System.Collections.Concurrent": "4.0.12",
        "System.Text.RegularExpressions": "4.1.0",
        "System.Xml.XmlSerializer": "4.0.11",
        "System.Reflection.Extensions": "4.0.1",
        "System.Reflection.TypeExtensions": "4.1.0",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Diagnostics.Process": "4.1.0",
        "System.Diagnostics.StackTrace": "4.0.1",
        "System.Threading": "4.0.11",
        "System.Threading.Thread": "4.0.0",
        "System.Globalization": "4.0.11",
        "System.Security.Cryptography.Algorithms": "4.2.0"
      }
    }
  }
}
